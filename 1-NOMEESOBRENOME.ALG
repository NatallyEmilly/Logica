Algoritmo "nomeesobrenome"

Var
   nome, sobrenome, nomecompleto: caractere
Inicio
      escreva("Digite o primeiro nome: ")
      leia(nome)
      escreva("Digite o sobrenome: ")
      leia(sobrenome)
      nomecompleto <- nome + " " + sobrenome
      escreva("O nome compelto é: " + nomeCompleto)
Fimalgoritmo